<!DOCTYPE html>
<html>
  <head>
  <% require "./app/poros/github_repo.rb" %>
   <%x = GithubRepo.new(GitHubService.repo_page, GitHubService.contributor_page)%>
    <title>LittleEtsyShop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <% flash.each do |type, msg| %>
      <div>
        <%= msg %>
      </div>
    <% end %>
  </head>

  <body>
    <%= yield %>
  </body>
  <div id = 'api'>
  <%= x.name %>
   <% x.contributors.each do |cont|%>
   <p> <%="Contributor: #{cont[0]} Commits: #{cont[1]}" %></p>
   <%end%>
   <div>
</html>
